:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x7fffeb1ccf80 .scope module, "top_tb" "top_tb" 2 3;
 .timescale -9 -9;
v0x7fffeb1df590_0 .var "a", 0 0;
v0x7fffeb1df660_0 .var "b", 0 0;
v0x7fffeb1df730_0 .net "c", 0 0, L_0x7fffeb1df830;  1 drivers
S_0x7fffeb1cd110 .scope module, "t0" "top" 2 21, 3 1 0, S_0x7fffeb1ccf80;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "c";
L_0x7fffeb1df830 .functor AND 1, v0x7fffeb1df590_0, v0x7fffeb1df660_0, C4<1>, C4<1>;
v0x7fffeb194cf0_0 .net "a", 0 0, v0x7fffeb1df590_0;  1 drivers
v0x7fffeb1df3b0_0 .net "b", 0 0, v0x7fffeb1df660_0;  1 drivers
v0x7fffeb1df470_0 .net "c", 0 0, L_0x7fffeb1df830;  alias, 1 drivers
    .scope S_0x7fffeb1ccf80;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffeb1df590_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffeb1df660_0, 0, 1;
T_0.0 ;
    %vpi_func 2 14 "$random" 32 {0 0 0};
    %pushi/vec4 100, 0, 32;
    %mod;
    %pad/u 64;
    %muli 1, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %load/vec4 v0x7fffeb1df590_0;
    %inv;
    %store/vec4 v0x7fffeb1df590_0, 0, 1;
    %vpi_func 2 16 "$random" 32 {0 0 0};
    %pushi/vec4 100, 0, 32;
    %mod;
    %pad/u 64;
    %muli 1, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %load/vec4 v0x7fffeb1df660_0;
    %inv;
    %store/vec4 v0x7fffeb1df660_0, 0, 1;
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_0x7fffeb1ccf80;
T_1 ;
    %vpi_call 2 29 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 30 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffeb1ccf80 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x7fffeb1ccf80;
T_2 ;
    %delay 1000, 0;
    %vpi_call 2 35 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
